{
  "name": "mfe-seed",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "prepare": "node .husky/install.mjs",
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "vitest --config vite.config.js run --coverage",
    "lint": "eslint src",
    "check": "prettier --ignore-path .gitignore --check src/** --log-level warn",
    "check:overwrite": "prettier --ignore-path .gitignore --write {src,test}/; eslint --fix src",
    "serve": "node --loader ts-node/esm server.ts"
  },
  "devDependencies": {
    "@hono/node-server": "^1.14.3",
    "@types/node": "^22.15.21",
    "@types/nunjucks": "^3.2.6",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "hono": "^4.7.10",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "nunjucks": "^3.2.4",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.89.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.37.0",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  }
}
