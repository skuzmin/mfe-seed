{
  "name": "mfe-seed",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "prepare": "node .husky/install.mjs",
    "dev:fe": "vite",
    "dev:be": "NODE_NO_WARNINGS=1 node --experimental-strip-types --env-file=./src/.env ./src/backend/index.ts",
    "build": "rm -rf dist && vite build",
    "test": "vitest --config vite.config.js run --coverage",
    "lint": "eslint src",
    "check": "prettier --ignore-path .gitignore --check src/** --log-level warn",
    "check:fix": "prettier --ignore-path .gitignore --write {src,test}/; eslint --fix src"
  },
  "devDependencies": {
    "@hono/node-server": "^1.14.3",
    "@types/node": "^22.15.21",
    "@types/nunjucks": "^3.2.6",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "highlight.js": "^11.11.1",
    "hono": "^4.7.10",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.0",
    "jsdom": "^26.1.0",
    "markdown-it": "^14.1.0",
    "markdown-it-anchor": "^9.2.0",
    "nunjucks": "^3.2.4",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.89.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.37.0",
    "vite": "^6.3.5",
    "vite-plugin-static-copy": "^3.1.1",
    "vitest": "^3.2.4"
  }
}
